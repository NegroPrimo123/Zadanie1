using System;

class Program
{
    static void Main()
    {
        // Внешний цикл для строк таблицы
        for (int i = 1; i <= 9; i++)
        {
            // Внутренний цикл для столбцов таблицы
            for (int j = 1; j <= 9; j++)
            {
                // Выводим результат умножения в нужном формате
                Console.Write($"{i} x {j} = {i * j}\t");
            }
            // Переход на новую строку после завершения внутреннего цикла
            Console.WriteLine();
        }
    }
}
